function init(){var c=0;var b=document.getElementById("J_TabBar");var d=b.getElementsByTagName("a");for(var a=0;a<d.length;a++){if(d[a].getAttribute("data-index")==1&&d[a].href.indexOf("#reviews")!==-1){c=1;simulate(d[a],"click");break}}if(c==1){setTimeout("clickNext()",3000)}else{ww.endOfPage();return true}}function clickNext(){var b=0;var c=0;ww.downloadSource();var e=document.getElementById("J_Pagination");b=1;var d=e.getElementsByTagName("a");for(var a=0;a<d.length;a++){if(d[a].className=="page-next"){c=1;simulate(d[a],"click");break}}if(b==0||b==1&&c==0){ww.endOfPage();return true}setTimeout("clickNext()",1000)}setTimeout("init()",2000);